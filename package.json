{
  "name": "polyonic",
  "productName": "Polyonic App",
  "description": "An Ionic project",
  "version": "2.2.3",
  "author": {
    "name": "Your Name",
    "email": "email@your-email.com",
    "url": "http://yourwebsite.com"
  },
  "repository": "https://github.com/paulsutherland/polyonic",
  "copyright": "Â© 2018, Your Name",
  "private": true,
  "main": "electron.js",
  "scripts": {
    "build:electron": "ng build --base-href ./",
    "build:electron:dev": "npm run build:electron -- -c dev",
    "build:electron:prod": "npm run build:electron -- -c production",
    "electron:dev": "npm-run-all -p ng:serve electron:serve",
    "electron:dev:debug": "npm-run-all -p ng:serve electron:serve:debug",
    "electron:dev:debug-live": "npm-run-all -p ng:serve electron:serve:debug-live",
    "electron:serve": "wait-on http-get://localhost:4200/ && run-electron . --serve",
    "electron:serve:debug": "wait-on http-get://localhost:4200/ && run-electron --inspect-brk=9229 . --serve",
    "electron:serve:debug-live": "wait-on http-get://localhost:4200/ && nodemon --watch electron.js --exec run-electron --inspect-brk=9229 . --serve",
    "electron:local": "npm run build:electron:prod && run-electron .",
    "electron:linux": "npm run build:electron:prod && npx electron-builder build --linux",
    "electron:windows": "npm run build:electron:prod && npx electron-builder build --windows",
    "electron:mac": "npm run build:electron:prod && npx electron-builder build --mac",
    "ionic": "npm run ng:serve:web",
    "emulate:ios-dev": "node config/disable-deploy && ionic cordova emulate ios --l -- --buildFlag='-UseModernBuildSystem=0'",
    "run:ios-dev": "node config/disable-deploy && ionic cordova run ios --l --device -- --buildFlag='-UseModernBuildSystem=0'",
    "emulate:ios": "node config/disable-deploy && ionic cordova emulate ios -- --buildFlag='-UseModernBuildSystem=0'",
    "run:ios": "node config/disable-deploy && ionic cordova run ios --device -- --buildFlag='-UseModernBuildSystem=0'",
    "release:ios": "node config/enable-deploy && ionic cordova build ios --prod --release -- --buildFlag='-UseModernBuildSystem=0'",
    "emulate:android-dev": "node config/disable-deploy && ionic cordova emulate android --l",
    "run:android-dev": "node config/disable-deploy && ionic cordova run android --device --l",
    "emulate:android": "node config/disable-deploy && ionic cordova emulate android",
    "run:android": "node config/disable-deploy && ionic cordova run android --device",
    "release:android": "node config/enable-deploy && ionic cordova build android --prod --release ",
    "ng": "ng",
    "ng:serve": "ng serve",
    "ng:serve:web": "ng serve -o",
    "precommit": "node config/enable-deploy && git add config.xml",
    "test": "ng test",
    "e2e": "ng e2e",
    "xcode": "open -a Xcode platforms/ios/"
  },
  "dependencies": {
    "@angular/common": "8.2.2",
    "@angular/core": "8.2.2",
    "@angular/forms": "8.2.2",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "8.2.2",
    "@angular/platform-browser-dynamic": "8.2.2",
    "@angular/router": "8.2.2",
    "@ion-phaser/core": "^1.2.0",
    "@ionic-native/core": "5.12.0",
    "@ionic-native/splash-screen": "5.12.0",
    "@ionic-native/status-bar": "5.12.0",
    "@ionic/angular": "4.7.4",
    "cordova-android": "8.0.0",
    "cordova-ios": "5.0.1",
    "cordova-plugin-device": "2.0.3",
    "cordova-plugin-ionic-keyboard": "2.1.3",
    "cordova-plugin-ionic-webview": "4.1.1",
    "cordova-plugin-splashscreen": "5.0.3",
    "cordova-plugin-statusbar": "2.4.3",
    "cordova-plugin-whitelist": "1.3.4",
    "cordova-sqlcipher-adapter": "0.4.1",
    "core-js": "3.2.1",
    "ngx-electron": "2.1.1",
    "phaser": "^3.20.1",
    "polyonic-secure-pouch": "1.0.4",
    "pouchdb": "7.1.1",
    "pouchdb-adapter-cordova-sqlite": "2.0.6",
    "run-electron": "1.0.0",
    "rxjs": "6.5.2",
    "zone.js": "0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.802.2",
    "@angular-devkit/build-angular": "0.802.2",
    "@angular-devkit/core": "8.2.2",
    "@angular-devkit/schematics": "8.2.2",
    "@angular/cli": "8.2.2",
    "@angular/compiler": "8.2.2",
    "@angular/compiler-cli": "8.2.2",
    "@angular/language-service": "8.2.2",
    "@ionic/angular-toolkit": "2.0.0",
    "@types/jasmine": "3.4.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.7.2",
    "codelyzer": "5.1.0",
    "electron": "9.4.0",
    "electron-builder": "21.2.0",
    "husky": "3.0.4",
    "jasmine-core": "3.4.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "4.2.0",
    "karma-chrome-launcher": "3.1.0",
    "karma-coverage-istanbul-reporter": "2.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "nodemon": "^1.19.2",
    "npm-run-all": "4.1.5",
    "protractor": "5.4.2",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "typescript": "3.5.3",
    "wait-on": "3.3.0"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-sqlcipher-adapter": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}